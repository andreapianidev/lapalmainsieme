// Funzioni helper per filtrare i luoghi
export const getPlacesByType = (type) => {
  return places.filter(place => place.type === type);
};

export const getRomanticPlaces = () => {
  return places.filter(place => place.romantic === true);
};

export const getPlacesByDifficulty = (difficulty) => {
  return places.filter(place => place.difficulty === difficulty);
};

